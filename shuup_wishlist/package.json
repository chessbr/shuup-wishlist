{
  "name": "shuup-wishlist",
  "version": "0.3.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:js": "parcel watch --no-hmr -d static/shuup_wishlist/js -o scripts static_src/js/index.js",
    "watch:styles": "parcel watch --no-hmr -d static/shuup_wishlist/css static_src/less/style.less",
    "watch": "concurrently --kill-others-on-fail npm:watch:*",
    "build:js": "parcel build -d static/shuup_wishlist/js -o scripts static_src/js/index.js",
    "build:styles": "parcel build -d static/shuup_wishlist/css static_src/less/style.less",
    "build": "npm i && concurrently --kill-others-on-fail npm:build:*"
  },
  "shuup": {
    "static_build": true
  },
  "author": "",
  "license": "Proprietary",
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.1.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.6.1",
    "less": "^3.8.1",
    "parcel-bundler": "^1.10.3",
    "postcss-modules": "^1.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3"
  }
}
